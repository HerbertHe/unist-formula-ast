{
  "name": "unist-formula-ast",
  "version": "1.2.1",
  "description": "Excel formula ast to implement unist specification",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm gen:dev\" \"pnpm src:dev\"",
    "build": "pnpm gen:build && pnpm src:build",
    "src:dev": "tsup --watch",
    "src:build": "tsup",
    "gen:dev": "peggy -w -c peggy.config.cjs",
    "gen:build": "peggy -c peggy.config.cjs",
    "test": "vitest"
  },
  "keywords": [
    "unist",
    "excel formula",
    "ast",
    "peggy"
  ],
  "author": "Herbert He<Herbert.He0229@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "concurrently": "^8.2.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@codemirror/lint": "^6.8.0",
    "@codemirror/state": "^6.4.1",
    "peggy": "^4.0.2",
    "ts-pegjs": "^4.2.1",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  }
}
